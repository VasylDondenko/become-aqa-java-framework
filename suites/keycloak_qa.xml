<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Suite" data-provider-thread-count="3">
    <test thread-count="1" name="Test" parallel="methods">

        <parameter name="env" value="qa" />

        <classes>
            <class name="api.keycloak.KeycloakApiTests">
                <methods>
                    <include name="checkUserExists"/>
                    <include name="checkQuantityUsersMoreThanOne"/>
                    <include name="checkSpecificUserInUsers"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>